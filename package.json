{
  "name": "interntracker",
  "version": "1.0.0",
  "description": "Intern Tracker ystem",
  "type": "module",
  "scripts": {
    "bev:quiet": "bun routify -c bev:vite:quiet ",
    "build": "bunx routify && bunx vite build",
    "cssdev": "run-p csswatch cssstart",
    "cssstart": "vite src/template --open --host",
    "csstemplate": "live-server src/template  ",
    "csswatch": "npx tailwindcss -i src/template/css/main.css -o src/template/css/output.css --watch",
    "dev": "vite dev --open --host 127.0.0.1",
    "dev:vite": "vite --open --host",
    "fix": "pnpx eslint --fix \"{,!(node_modules|public|dist|.routify)/**/}*.{js,svelte}\"",
    "format": "pnpx prettier --write --plugin-search-dir=. \"{,!(node_modules|public|dist|.routify)/**/}*.{js,svelte,json,html}\"",
    "pbuild": "vite build",
    "pprebuild": "test \"$CI\"=true && npx pnpm install --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "prebuild": "test \"$CI\"=true && bun  install --store=node_modules/.pnpm-store || echo skiping bun install",
    "preview": "vite preview",
    "routify": "routify",
    "routifyb": "routify --clearRoutifyDir",
    "test:e2e": "playwright test",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:codegen": "playwright codegen http://localhost:5173",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:install": "playwright install",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:report": "playwright show-report",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:webkit": "playwright test --project=webkit"
  },
